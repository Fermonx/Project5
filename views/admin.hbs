    <div id="traveltable" class="login-wrapper d-flex align-items-center table ">
        <div class="container form-login">
            <h2 class="d-flex justify-content-center">Administración de Viajes</h2>
            <table class="table table-condensed table-responsive">
                <thead>
                <tr>
                    <th>ID</th>
                    <th>Viaje</th>
                    <th>Descripción</th>
                    <th>Precio</th>
                    <th>Tipo</th>
                    <th>Activo</th>
                </tr>
                </thead>
                <tbody>
                {{#each retrieveTravel}}
                        <tr>
                            <td>{{id}}</td>
                            <td>{{travel}}</td>
                            <td>{{description}}</td>
                            <td>{{price}}</td>
                            <td>{{tipo}}</td>

                            <td><input type="checkbox"  onclick='window.location.assign("/admintable/hideTravel/{{id}}")' {{#if active}}checked{{/if}}></td>
                            <td><button type="button"  onclick='window.location.assign("/admintable/travelDelete/{{id}}")' class="btn btn-danger"><i class="fas fa-trash-alt"></i></button></td>
                        </tr>
                {{/each}}

                </tbody>
                <div style="margin-bottom: 10px;">

                </div>
            </table>
            <button type="submit" data-toggle="modal" data-target="#createModal" class="btn btn-success align-items-end">Crear</button>
            <button type="button" class="btn btn-info align-items-end" style="margin-left: 10px;">Modificar</button>
        </div>
    </div>

    <div class="modal fade " id="createModal" tabindex="-1" role="textbox" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">Agregar Viaje</h5>
                </div>
                <div class="modal-body">
                    <form method="post" action="/admintable/create">
                        <div class="container">
                            <div class="row">
                                <div class="col-12 ">
                                    <input type="text" placeholder="Nombre" name="travel" required>
                                    <br/>
                                    <br/>
                                    <input type="text" placeholder="Descripcion" name="description" required>

                                </div>
                                <div class="col-12 ">
                                    <br/>
                                    <input type="text" placeholder="Precio" name="price" required>
                                    <br/>
                                    <br/>
                                    <input type="text" placeholder="Tipo" name="tipo" required>
                                </div>
                            </div>
                        </div>
                        <br/>
                        <button type="submit" class="btn btn-success">Crear</button>
                    </form>
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Cerrar</button>
                </div>
                <div class="modal-footer">

                </div>
            </div>
        </div>
    </div>

